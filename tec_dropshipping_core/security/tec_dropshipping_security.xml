<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_tec_ecommerce_suite" model="ir.module.category">
        <field name="name">Tec eCommerce Suite</field>
        <field name="sequence">20</field>
    </record>

    <record id="group_tec_ecommerce_suite_user" model="res.groups">
        <field name="name">Usuario Tec Suite</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_tec_ecommerce_suite_manager" model="res.groups">
        <field name="name">Administrador Tec Suite</field>
        <field name="implied_ids" eval="[(4, ref('group_tec_ecommerce_suite_user'))]"/>
    </record>
</odoo>
